#include <iostream>
#include <string>
using namespace std;



void ReadNumbers(int& A, int& B)
{
	
	cout << "Please Enter number 1 :" << endl;
	cin >> A;

	cout << "Please Enter nymber 2 :" << endl;
	cin >> B;


}


int calculateArea(int A, int B)
{
	return A * B;
	
}

void PrintArea(int area)
{
	cout << " The Area is :" << area << endl;

	
}

int main()
{
	int A, B;
	
	ReadNumbers(A, B);
	
	PrintArea(calculateArea(A,B));
}
