#include <iostream>
#include <string>
using namespace std;



void ReadNumbers(int& num1, int& num2)
{
	
	cout << "Please Enter number 1 :" << endl;
	cin >> num1;

	cout << "Please Enter nymber 2 :" << endl;
	cin >> num2;

}


int checkMax(int num1, int num2)
{
	
	if (num1 > num2)
		return num1;
	else
		return num2;
}

void PrintMax(int themax)
{
	cout << " The max Number is :" << themax;

}

int main()
{
	int num1, num2;
	ReadNumbers(num1, num2);
	PrintMax(checkMax(num1, num2));
}
