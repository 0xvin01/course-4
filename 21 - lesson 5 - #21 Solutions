#include <iostream>
#include <string>
#include <cmath>
using namespace std;



float ReadNumbers()
{
	float L;
	cout << "Please Enter L  :" << endl;
	cin >> L;
	return L;
	
}


float calculateArea(float L)
{
	const float pi = 3.1415926535;
	float result = pow(L, 2) / (4 * pi);
	return result;
	
}

void PrintArea(float area)
{
	cout << " The Area is :" << area << endl;

	
}

int main()
{
	
	
		PrintArea(calculateArea(ReadNumbers()));

		return 0;
}
